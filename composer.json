{
    "name": "frame-php/application",
    "type": "project",
    "license": "MIT",
    "description": "Main FramePHP application and all dependencies",
    "authors": [
        {
            "name": "Hilkiah Makemo",
            "email": "hilkiah.makemo@skywest.com"
        }
    ],
    "require-dev": {
        "maximebf/debugbar": "^1.15",
        "fzaninotto/faker": "^1.7"
    },
    "require": {
        "frame-php/module": "^1.0"
    },
    "extra":{
        "installer-paths": {
            "./{$name}": ["frame-php/module-app", "frame-php/module-sys"]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "FramePHP\\": "vendor/frame-php/framework/src/"
        },
        "files": [ "sys/autoload.php" ]
    },
    "scripts": {
        "post-install-cmd": "Installer::postInstall",
        "post-update-dump": "Installer::postUpdate"
    },
    "minimum-stability": "dev"
}
